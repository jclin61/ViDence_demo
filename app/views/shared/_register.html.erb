<%= simple_form_for @user , url: sessions_path do |f| %>
	<%= f.input :username %>
	<%= f.input :email %>
	<%= f.input :password %>
	<%= f.input :password_confirmation %>
	<%= f.input :first_name %>
	<%= f.input :last_name %>
	<%= f.input :location %>
	<%= f.button :submit, value: 'Register' %>
<% end %>